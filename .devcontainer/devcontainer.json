{
 "name": "Node.js",
 "build": {
  "dockerfile": "Dockerfile",
  "args": {
   "VARIANT": "16",
   "USER": "node",
   "GROUP": "node"
  }
 },
 "settings": {
  "search.exclude": {
   "**/.pnp.*": true
  },
  "editor.codeActionsOnSave": {
   "source.eslint.fixAll": true
  }
 },
 "remoteUser": "node",
 "forwardPorts": [3000, 3001]
}
